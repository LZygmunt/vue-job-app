{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": false,
  "singleQuote": true,
  "printWidth": 120,
  "singleAttributePerLine": true,
  "trailingComma": "all",
  "arrowParens": "always",
  "experimentalOperatorPosition": "start",
  "bracketSpacing": true,
  "bracketSameLine": false
}
