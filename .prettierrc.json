{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": false,
  "singleQuote": true,
  "printWidth": 100,
  "proseWrap": "never",
  "singleAttributePerLine": true,
  "trailingComma": "all",
  "arrowParens": "always",
  "experimentalOperatorPosition": "start",
  "bracketSpacing": true,
  "bracketSameLine": false
}
